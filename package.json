{
  "name": "@pengzhanbo/utils",
  "version": "1.0.8",
  "keywords": [
    "utils"
  ],
  "homepage": "https://github.com/pengzhanbo/utils",
  "bugs": "https://github.com/pengzhanbo/utils/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/pengzhanbo/utils.git"
  },
  "license": "MIT",
  "author": "pengzhanbo <volodymyr@foxmail.com> (https://github.com/pengzhanbo)",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs"
    },
    "./package.json": "./package.json",
    "./is": {
      "require": "./dist/cjs/is.cjs",
      "import": "./dist/esm/is.mjs"
    },
    "./array": {
      "require": "./dist/cjs/array.cjs",
      "import": "./dist/esm/array.mjs"
    },
    "./clone": {
      "require": "./dist/cjs/clone.cjs",
      "import": "./dist/esm/clone.mjs"
    },
    "./equal": {
      "require": "./dist/cjs/equal.cjs",
      "import": "./dist/esm/equal.mjs"
    },
    "./event": {
      "require": "./dist/cjs/event.cjs",
      "import": "./dist/esm/event.mjs"
    },
    "./function": {
      "require": "./dist/cjs/function.cjs",
      "import": "./dist/esm/function.mjs"
    },
    "./math": {
      "require": "./dist/cjs/math.cjs",
      "import": "./dist/esm/math.mjs"
    },
    "./object": {
      "require": "./dist/cjs/object.cjs",
      "import": "./dist/esm/object.mjs"
    },
    "./promise": {
      "require": "./dist/cjs/promise.cjs",
      "import": "./dist/esm/promise.mjs"
    },
    "./string": {
      "require": "./dist/cjs/string.cjs",
      "import": "./dist/esm/string.mjs"
    },
    "./time": {
      "require": "./dist/cjs/time.cjs",
      "import": "./dist/esm/time.mjs"
    },
    "./url": {
      "require": "./dist/cjs/url.cjs",
      "import": "./dist/esm/url.mjs"
    }
  },
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "eslint .",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run --reporter verbose",
    "release": "bumpp package.json --commit --push --tag && npm publish --access public"
  },
  "prettier": "@pengzhanbo/prettier-config",
  "dependencies": {
    "throttle-debounce": "^5.0.0"
  },
  "devDependencies": {
    "@pengzhanbo/eslint-config": "^0.3.15",
    "@pengzhanbo/prettier-config": "^0.3.15",
    "@types/node": "^20.3.2",
    "@types/throttle-debounce": "^5.0.0",
    "bumpp": "^9.1.1",
    "eslint": "^8.43.0",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3",
    "unbuild": "^1.2.1",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "packageManager": "pnpm@8.6.5"
}
