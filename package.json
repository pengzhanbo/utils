{
  "name": "@pengzhanbo/utils",
  "version": "1.0.0",
  "keywords": [
    "utils"
  ],
  "homepage": "https://github.com/pengzhanbo/utils",
  "bugs": "https://github.com/pengzhanbo/utils/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/pengzhanbo/utils.git"
  },
  "license": "MIT",
  "author": "pengzhanbo <volodymyr@foxmail.com> (https://github.com/pengzhanbo)",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "lint": "eslint .",
    "prepublishOnly": "pnpm run build",
    "test": "vitest --reporter verbose",
    "release": "bumpp --commit --push --tag && npm publish --access public"
  },
  "prettier": "@pengzhanbo/prettier-config",
  "dependencies": {
    "throttle-debounce": "^5.0.0"
  },
  "devDependencies": {
    "@pengzhanbo/eslint-config": "^0.3.14",
    "@pengzhanbo/prettier-config": "^0.3.14",
    "@types/node": "^20.3.1",
    "@types/throttle-debounce": "^5.0.0",
    "bumpp": "^9.1.1",
    "eslint": "^8.42.0",
    "prettier": "^2.8.8",
    "tsup": "^6.7.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.32.0"
  },
  "packageManager": "pnpm@8.6.0"
}
