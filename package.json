{
  "name": "@pengzhanbo/utils",
  "version": "1.0.5",
  "keywords": [
    "utils"
  ],
  "homepage": "https://github.com/pengzhanbo/utils",
  "bugs": "https://github.com/pengzhanbo/utils/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/pengzhanbo/utils.git"
  },
  "license": "MIT",
  "author": "pengzhanbo <volodymyr@foxmail.com> (https://github.com/pengzhanbo)",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./package.json": "./package.json",
    "./is": {
      "require": "./dist/cjs/is.js",
      "import": "./dist/esm/is.js"
    },
    "./array": {
      "require": "./dist/cjs/array.js",
      "import": "./dist/esm/array.js"
    },
    "./clone": {
      "require": "./dist/cjs/clone.js",
      "import": "./dist/esm/clone.js"
    },
    "./equal": {
      "require": "./dist/cjs/equal.js",
      "import": "./dist/esm/equal.js"
    },
    "./event": {
      "require": "./dist/cjs/event.js",
      "import": "./dist/esm/event.js"
    },
    "./function": {
      "require": "./dist/cjs/function.js",
      "import": "./dist/esm/function.js"
    },
    "./math": {
      "require": "./dist/cjs/math.js",
      "import": "./dist/esm/math.js"
    },
    "./object": {
      "require": "./dist/cjs/object.js",
      "import": "./dist/esm/object.js"
    },
    "./promise": {
      "require": "./dist/cjs/promise.js",
      "import": "./dist/esm/promise.js"
    },
    "./string": {
      "require": "./dist/cjs/string.js",
      "import": "./dist/esm/string.js"
    },
    "./time": {
      "require": "./dist/cjs/time.js",
      "import": "./dist/esm/time.js"
    },
    "./url": {
      "require": "./dist/cjs/url.js",
      "import": "./dist/esm/url.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist",
    "*.d.ts",
    "!*.tsbuildinfo"
  ],
  "scripts": {
    "build": "pnpm clean && pnpm build:tsc",
    "clean": "rimraf dist",
    "build:tsc": "tsc -b tsconfig.build.json",
    "lint": "eslint .",
    "prepublishOnly": "pnpm run build",
    "test": "vitest run --reporter verbose",
    "release": "bumpp package.json --commit --push --tag && npm publish --access public"
  },
  "prettier": "@pengzhanbo/prettier-config",
  "dependencies": {
    "throttle-debounce": "^5.0.0"
  },
  "devDependencies": {
    "@pengzhanbo/eslint-config": "^0.3.15",
    "@pengzhanbo/prettier-config": "^0.3.15",
    "@types/node": "^20.3.2",
    "@types/throttle-debounce": "^5.0.0",
    "bumpp": "^9.1.1",
    "eslint": "^8.43.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.32.2"
  },
  "packageManager": "pnpm@8.6.5"
}
